extends layout

block content
  #nag
    p
      if prevPage
        a(href=prevPage) <<
      if nextPage
        a(href=nextPage) >>
  each val in years
    h1= val.year
    p 
      each photo in val.photos
        - var thumbnailLink = "/html/thumbnails/" + photo.albumName + "/" + photo.photoName
        - var photolLink = "/html/albums/" + photo.albumName + "/" + photo.photoName
        a(href=photolLink)
          img(src=thumbnailLink, width=50, height=50)
